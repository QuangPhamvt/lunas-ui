{
  "name": "@customafk/e-commerce-ui",
  "private": false,
  "version": "0.0.0-j",
  "type": "module",
  "homepage": "https://docs.customafk.com",
  "repository": {
    "url": "git+https://github.com/QuangPhamvt/e-commerce-ui.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./tailwindTheme": {
      "import": "./dist/tailwindTheme.js",
      "require": "./dist/tailwindTheme.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fmt": "prettier --check \"./packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "fmt:fix": "prettier --write \"./packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.424.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-interactions": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/addon-onboarding": "^8.2.7",
    "@storybook/addon-themes": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/react": "^8.2.7",
    "@storybook/react-vite": "^8.2.7",
    "@storybook/test": "^8.2.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "storybook": "^8.2.7",
    "tailwindcss": "^3.4.7",
    "tsup": "^8.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^4.3.2",
    "@types/bun": "latest"
  },
  "description": "This is a e-commerce UI library for CustomAFK",
  "bugs": {
    "url": "https://github.com/QuangPhamvt/e-commerce-ui/issues"
  },
  "author": "CustomAFK",
  "license": "ISC"
}
